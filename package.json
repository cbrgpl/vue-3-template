{
  "name": "front_v2",
  "version": "0.0.1",
  "lint-staged": {
    "*.{css,scss,sass}": "prettier --write",
    "*.{js,vue,jsx,cjs,mjs}": "eslint --config .eslintrc.js --ignore-path .gitignore",
    "*.{css,scss,sass,vue}": "stylelint --ignore-path .gitignore"
  },
  "scripts": {
    "check-types:once": "vue-tsc",
    "dev": "vite",
    "build": "sh ./scripts/build.sh",
    "preview": "vite preview --port 4173",
    "coverage": "vitest run --coverage",
    "test:unit": "vitest",
    "test:unit:once": "vitest run",
    "lintfix": "npm run lint:js -- --fix && npm run lint:style -- --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path  .gitignore",
    "lint:style": "prettier **/*.{scss,css} --write --ignore-unknown && stylelint **/*.{vue,css} --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "pinia": "^2.0.33",
    "ts-deepmerge": "^6.0.3",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@iconify/vue": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/ui": "^0.25.8",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.6",
    "autoprefixer": "^10.4.13",
    "babel-preset-vue": "^2.0.2",
    "c8": "^7.12.0",
    "eslint": "^8.27.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.0",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "prettier": "2.8.8",
    "sass": "^1.63.6",
    "stylelint": "^15.9.0",
    "stylelint-config-hudochenkov": "^9.0.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.6.5"
  }
}
